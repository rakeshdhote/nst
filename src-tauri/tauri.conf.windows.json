{
  "tauri": {
    "bundle": {
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "http://timestamp.digicert.com",
        "wix": {
          "language": "en-US"
        },
        "webviewInstallMode": {
          "type": "downloadBootstrapper"
        },
        "signing": {
          "type": "file",
          "certFile": "windows-cert.pfx",
          "password": "$WINDOWS_SIGN_CERT_PASSWORD"
        }
      }
    }
  }
}
